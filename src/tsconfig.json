{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
    },
    "module": "commonjs",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "allowJs": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "ES5",
    "experimentalDecorators": true,
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}